#include<iostream>
#include<bits/stdc++.h>
#include<vector>
using namespace std;


int Divide(int dvdn,int dvsr)
{
      if(dvdn==dvsr)
        return 1;
      int sign= 1;
      if(dvdn>=0 && dvsr<0)
            sign =0;
      if(dvdn<0  && dvsr<0)
            sign =0;
      int n=abs(dvdn);
      int d=abs(dvsr);
       if(d>n)
            return 0;
        int ans=0;
       while(n>=d){
        int cnt=0;
            while(n>=(d<<(cnt+1)))
                cnt++;
           ans+= 1<<cnt;
           n=n-(d*(1<<cnt)) ;
       }
       if(ans>=pow(2,31) && sign==1)
            return INT_MAX;
       if(ans>=pow(2,31) && sign==0)
            return INT_MIN;
        return sign? ans : (-1*ans);
}

int main()
{
  int divident,divisor;
  cin>>divident>>divisor;
  int a=Divide(divident,divisor);
  cout<<a;
    return 0;
}

