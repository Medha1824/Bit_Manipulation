#include<iostream>
#include<bits/stdc++.h>
using namespace std;

long long decimal2Binary(int n){
    long long ans = 0;
    int rem, i = 1;
    while(n!=0){
        rem = n % 2;
        ans += rem*i;
        i = i * 10;
        n = n / 2;
    }
    return ans;
}
long long binary2decimal(int n){
    int i = 0, num= 0;
    while (n!=0)
    {
        int a = n % 10;
        num += a * pow(2,i);

        n /= 10;
        i++;
    }
    return num;
}
int main()
{
    int n,i;
    cin >> n >> i;
    int a=decimal2Binary(n);
    int b=(a|(1<<i));
    cout<< binary2decimal(b)<<endl;
    return 0;
}
